{
  "name": "@supabase/pg-diff",
  "version": "0.0.0",
  "description": "PostgreSQL migrations made easy",
  "type": "module",
  "main": "./dist/core/index.js",
  "types": "./dist/core/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    }
  },
  "bin": {
    "pg-diff": "./dist/src/cli/bin/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "imports": {
    "#test": "./test.ts"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "cli:dev": "node src/cli/bin/cli.ts",
    "dev": "docker compose up",
    "check": "biome check . --error-on-warnings",
    "format": "biome format . --error-on-warnings",
    "lint": "biome lint . --error-on-warnings",
    "knip": "knip",
    "test": "vitest run",
    "test:unit": "vitest --project unit",
    "test:integration": "vitest --project integration",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage --project=integration",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "pg",
    "postgres",
    "migrations",
    "diff",
    "pg-diff"
  ],
  "author": "Supabase",
  "license": "MIT",
  "packageManager": "pnpm@10.24.0",
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@tsconfig/node-ts": "^23.6.2",
    "@tsconfig/node24": "^24.0.3",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "4.0.15",
    "dedent": "^1.7.0",
    "knip": "^5.71.0",
    "testcontainers": "^11.9.0",
    "typescript": "^5.9.3",
    "vitest": "4.0.15"
  },
  "dependencies": {
    "@stricli/core": "^1.2.4",
    "debug": "^4.3.7",
    "postgres": "^3.4.7",
    "zod": "^4.1.13"
  }
}
