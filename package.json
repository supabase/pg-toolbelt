{
  "name": "@supabase/pg-delta",
  "version": "0.0.0",
  "description": "PostgreSQL migrations made easy",
  "type": "module",
  "sideEffects": false,
  "exports": "./dist/core/index.js",
  "bin": {
    "pgdelta": "./dist/cli/bin/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "pg",
    "postgres",
    "migrations",
    "diff",
    "pg-delta",
    "pgdelta"
  ],
  "author": "Supabase",
  "license": "MIT",
  "homepage": "https://github.com/supabase/pg-delta",
  "repository": {
    "type": "git",
    "url": "https://github.com/supabase/pg-delta.git"
  },
  "bugs": "https://github.com/supabase/pg-delta/issues",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "check-types": "tsc --noEmit",
    "format-and-lint": "biome check . --error-on-warnings",
    "knip": "knip",
    "pgdelta": "node src/cli/bin/cli.ts",
    "prepublishOnly": "pnpm build",
    "test": "vitest"
  },
  "dependencies": {
    "@stricli/core": "^1.2.4",
    "debug": "^4.3.7",
    "postgres": "^3.4.7",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@tsconfig/node-ts": "^23.6.2",
    "@tsconfig/node24": "^24.0.3",
    "@types/debug": "^4.1.12",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "4.0.15",
    "dedent": "^1.7.0",
    "knip": "^5.71.0",
    "testcontainers": "^11.9.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
