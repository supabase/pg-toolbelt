{
  "name": "pg-diff",
  "version": "0.0.0",
  "description": "PostgreSQL migrations made easy",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "imports": {
    "#test": "./test.ts"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "docker compose up",
    "check": "biome check . --error-on-warnings",
    "format": "biome format . --error-on-warnings",
    "lint": "biome lint . --error-on-warnings",
    "knip": "knip",
    "test": "vitest run",
    "test:unit": "vitest --project unit",
    "test:integration": "vitest --project integration",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage --project=integration",
    "typecheck": "tsc --noEmit",
    "test:supabase": "vitest run tests/supabase/supabase.test.ts"
  },
  "keywords": [
    "pg",
    "postgres",
    "migrations",
    "diff",
    "pg-diff"
  ],
  "author": "Supabase",
  "license": "MIT",
  "packageManager": "pnpm@10.18.1",
  "devDependencies": {
    "@biomejs/biome": "2.3.3",
    "@tsconfig/node-ts": "^23.6.1",
    "@tsconfig/node24": "^24.0.1",
    "@types/node": "^24.10.0",
    "@vitest/coverage-v8": "4.0.7",
    "dedent": "^1.7.0",
    "knip": "^5.67.1",
    "postgres": "^3.4.7",
    "testcontainers": "^11.8.0",
    "typescript": "^5.9.3",
    "vitest": "4.0.7"
  },
  "dependencies": {
    "zod": "^4.1.12"
  }
}
